programa

    declare a,b,c,d,e.

    escreva("Programa Teste").

    escreva ("Digite A").
    leia (a:Num).
    a := 2.
    escreva ("Digite B").
    leia (b:Num).
    b := 2.

    se (a<b)
    entao
        c := a + b + 1.

    senao
        c := a - b.
    fimse

    escreva ("C e igual a ").
    escreva (c).

    d := c * a + b.
    escreva ("D e igual a ").
    escreva (d).
fimprog.